{
  "name": "oichegamos",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:prd": "ng build --configuration production",
    "build:dev": "ng build --configuration development --base-href=/dev/",
    "dev:ssr": "ng run oichegamos:serve-ssr",
    "serve:ssr": "node dist/oichegamos/server/main.js",
    "build:ssr": "ng build && ng run oichegamos:server",
    "prerender": "ng run oichegamos:prerender",
    "serve:serverless": "serverless offline start",
    "build:serverless": "ng build && ng run oichegamos:serverless:production",
    "deploy:serverless": "npm run build:serverless && serverless deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.9",
    "@angular/common": "^16.2.9",
    "@angular/compiler": "^16.2.9",
    "@angular/core": "^16.2.9",
    "@angular/forms": "^16.2.9",
    "@angular/platform-browser": "^16.2.9",
    "@angular/platform-browser-dynamic": "^16.2.9",
    "@angular/platform-server": "^16.2.9",
    "@angular/router": "^16.2.9",
    "@nguniversal/express-engine": "^16.2.0",
    "aws-serverless-express": "^3.4.0",
    "bootstrap": "^5.1.3",
    "express": "^4.15.2",
    "ngx-bootstrap": "^11.0.2",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.6",
    "@angular/cli": "^16.2.6",
    "@angular/compiler-cli": "^16.2.9",
    "@nguniversal/builders": "^16.2.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^16.11.7",
    "jasmine-core": "~4.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "serverless": "^3.35.2",
    "serverless-api-compression": "^1.0.1",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-associate-waf": "^1.2.1",
    "serverless-offline": "^13.2.0",
    "typescript": "^5.1.3"
  }
}
